{
  "name": "casperapi",
  "version": "0.0.7-b4",
  "description": "js api for casper distributed fs infrastructure",
  "main": "dist/casper.node.js",
  "scripts": {
    "dev": "webpack -w --mode=development --hide-modules",
    "serve": "webpack-dev-server --config=webpack.playground-serve --mode=development",
    "build-play": "webpack --config=webpack.playground-serve --mode=production",
    "test": "jest",
    "codecov": "codecov",
    "build": "webpack --mode=production --hide-modules && npm run build-dev",
    "build-dev": "webpack --mode=development --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Casper-dev/casperjs.git"
  },
  "browser": "dist/casper.js",
  "author": "Casper Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Casper-dev/casperjs/issues"
  },
  "homepage": "https://github.com/Casper-dev/casperjs#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "codecov": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "koa": "^2.5.1",
    "koa-body": "^2.5.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "web3": "^1.0.0-beta.33",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "js-sha256": "^0.9.0",
    "request": "^2.85.0",
    "stream-length": "^1.0.2"
  }
}
